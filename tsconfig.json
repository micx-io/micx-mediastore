{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "declaration": true,
    "module": "es2015",
    "target": "es6",
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "lib": [
      "es2021",
      "dom"
    ],

  },
  "include": [
    "./src.fe/*"
  ],
  "exclude": [
    "./test",
    "*/dist/*"
  ]
}
